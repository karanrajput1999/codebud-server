
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}


model User {
  id String @id @default(uuid())
  username String 
  email String
  password String
  questions String[] @default([])
  answers String[] @default([])
  reputation Int @default(0)
}